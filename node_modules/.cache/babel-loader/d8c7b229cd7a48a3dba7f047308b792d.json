AA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAnBA;AAoBA,IAAA,MApBA,mBAoBA,KApBA,EAoBA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KAtBA;AAuBA,IAAA,aAvBA,yBAuBA,KAvBA,EAuBA;AAAA;;AACA;AACA,WAAA,eAAA,CAAA;AAAA,QAAA,UAAA,EAAA,IAAA;AAAA,QAAA,cAAA,EAAA;AAAA,iBAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AAAA;AAAA,OAAA,EAFA,CAGA;AACA,KA3BA;AA4BA,IAAA,cA5BA,0BA4BA,MA5BA,EA4BA;AACA;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,GAAA,MAAA;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,aAAA,MAAA,CAAA,YAAA;AACA,KAhCA;AAiCA,IAAA,OAjCA,mBAiCA,CAjCA,EAiCA;AACA,WAAA,cAAA,CAAA,CAAA,CAAA,MAAA;AACA;AAnCA,IArBA;AA0DA,EAAA,OA1DA,qBA0DA;AACA,QAAA,KAAA,MAAA,CAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,EAAA;AACA,WAAA,IAAA,GAAA,KAAA,KAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,CAAA;AACA;AACA,GA9DA,CA+DA;AACA;AACA;AACA;AACA;AACA;;AApEA,CAAA","sourcesContent":["<template>\n  <div class=\"create\">\n    <h2 class=\"create__title\">{{!this.$route.params.id ? 'Новая заметка:' : 'Изменить заметку'}}</h2>\n    <div class=\"create__form\" >\n      <input v-model=\"item.title\" name=\"todoName\" class ='create__input create__input_type_title' placeholder=\"Название\" />\n      <div class=\"create__tasks\" v-for=\"(t, index) in item.tasks\" :key=\"index\">\n          <button class=\"create__button create__button_type_completed\"></button>\n          <textarea ref=\"inputTask\" @input='onInput' v-model=\"item.tasks[index].title\"  class ='create__input create__input_type_task' placeholder=\"Задача\" />\n          <button class=\"create__button create__button_type_delete\" @click=\"() => onDeleteClick(index)\"></button> \n      </div>\n      <div class=\"create__change\">\n        <button ref=\"buttonAdd\" class=\"create__button create__button_type_add\" @click=\"onAddTaskClick\"></button>\n        <button class=\"create__button-save\" @click=\"onSaveClick\">Сохранить</button>\n      </div>\n      \n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapState } from 'vuex';\n\n\nexport default {\n  name: 'Create',\n  props: {\n    itemId: {\n      type: Number,\n      required: false\n    }\n  },\n  data() {\n    return {\n      // title: '',\n      // tasks: [''],\n      item: {\n        title: '',\n        tasks: [{title: '', completed: false}],\n      }\n    }\n  },\n  computed: {\n    ...mapState(['items']),\n  },\n  methods: {\n    ...mapActions([\n      'addItem', \n      'deleteItem', \n      'setPopupVisible',\n      'setItem'\n    ]),\n    onAddTaskClick() {\n      // this.tasks.push('')\n      this.item.tasks.push({title: '', completed: false})\n    },\n    onSaveClick() {\n      if (this.$route.path === '/create') {\n        this.addItem(this.item)\n      }\n      else if (this.$route.path.includes('/edit')) {\n        this.setItem({ itemId: this.itemId, item: this.item })\n      }\n      this.$router.push('/')\n    },\n    delete(index) {\n      this.tasks.splice(index, 1)\n    },\n    onDeleteClick(index){\n      // this.setPopupVisible({ visibility: true, deleteIndex: index })\n      this.setPopupVisible({ visibility: true, deleteFunction: () => this.delete(index) })\n      // this.tasks.splice(index, 1)\n    },\n    resizeTextArea(target) {\n      // console.log(target)\n      target.style.height ='auto'\n      target.style.height =`${target.scrollHeight}px`\n    },\n    onInput(e){\n      this.resizeTextArea(e.target)\n    },\n  },\n  mounted() {\n    if (this.$route.path.includes('/edit')) {\n      this.item = this.items[this.$route.params.id]\n    }\n  },\n  // updated() {\n  //   if (!this.$route.params.id) {\n  //     this.title = ''\n  //     this.tasks = ['']\n  //   }\n  // }\n}\n</script>\n\n<style>\n.create {\n  display: flex;\n  flex-direction: column;\n  width: 820px;\n}\n.create__title{\n  padding: 5px;\n}\n.create__input{\n  width: 100%;\n  max-width: 700px;\n  padding: 10px;\n  margin: 7px;\n  background-color: transparent;\n  box-sizing: border-box;\n  border: none;\n  border-bottom: 1px solid rgb(0, 0, 0, 0.2);\n}\n\n.create__input_type_title{\n  margin-left: 37px;\n}\n\n.create__input_type_task{\n  height: auto;\n  outline: none;\n  overflow: auto;\n}\n\n\n.create__form {\n  display: flex;\n  flex-direction: column;\n}\n\n.create__change{\n  display: flex;\n  justify-content: end;\n  align-items: center;\n  margin-top: 15px;\n\n}\n\n.create__button_type_add{\n  background-image: url('../assets/icons/addButton.png');\n  margin-right: 15px;\n\n}\n\n.create__button_type_delete{\n  margin-left: auto;\n  background-image: url('../assets/icons/delete.png');\n}\n\n.create__button_type_completed{\n  background-image: url('../assets/icons/completed.png');\n  width: 10px;\n  height: 10px;\n}\n\n.create__button-save{\n  max-width: 100px;\n  width: 100%;\n  height: 40px;\n  border: none;\n  background-color: rgb(196, 141, 247);\n  color: #fff;\n  cursor: pointer;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  border-radius: 25px;\n  margin-right: 76px;\n  align-self: flex-end;\n}\n\n\n.create__tasks {\n  display: flex;\n  position: relative;\n  flex-direction: row;\n}\n\n.create__button{\n  width: 30px;\n  height: 30px;\n  border-radius: 50%;\n  background-color: transparent;\n  border: 1px solid #0000008e;\n  cursor: pointer;\n  align-self:center;\n  background-position: center;\n  background-size: 16px 16px;\n  background-repeat: no-repeat;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n}\n\n</style>"],"sourceRoot":"src/components"}]}